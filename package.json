{
  "name": "react-npm-es6-boilerplate",
  "description": "A boilerplate for creating react npm packages",
  "version": "0.1.0",
  "author": "Robert Gonzales",
  "license": "MIT",
  "keywords": [
    "react",
    "es6",
    "npm",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "boilerplate",
    "starter",
    "publish"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robertgonzales/react-npm-es6-boilerplate.git"
  },
  "bugs": "https://github.com/robertgonzales/react-npm-es6-boilerplate/issues",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "prepublish": "babel --plugins 'transform-es2015-modules-umd' src --out-dir ./dist"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "component-playground": "^1.3.0",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "node-sass": "3.7.0",
    "postcss-loader": "0.8.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "lodash": "4.11.1",
    "react": "15.0.1",
    "react-dom": "15.0.1"
  }
}
